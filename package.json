{
  "name": "megadental-art",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "firebase": "^5.0.4",
    "flow": "^0.2.3",
    "material-design-icons": "^3.0.1",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.4.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.0",
    "react-ga": "^2.5.3",
    "react-images": "^0.5.17",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-tap-event-plugin": "^3.0.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "yarn flow && yarn build && firebase deploy;",
    "eject": "react-scripts eject",
    "flow": "flow --show-all-errors; test $? -eq 1 -o $? -eq 2",
    "flow:coverage": "node node_modules/flow-coverage-report/bin/flow-coverage-report.js -i 'src/**/*.js' -t html -t json -t text --threshold 90",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:e2e": "testcafe firefox src/tests/*.e2e.js"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "flow-bin": "^0.73.0",
    "flow-coverage-report": "^0.5.0",
    "testcafe": "^0.20.2"
  }
}
