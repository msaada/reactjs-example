{
  "name": "megadental-art",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "firebase": "^7.20.0",
    "flow": "^0.2.3",
    "react": "^16.13.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-images": "^0.5.17",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "yarn flow && yarn build && firebase deploy;",
    "eject": "react-scripts eject",
    "flow": "flow --show-all-errors; test $? -eq 1 -o $? -eq 2",
    "flow:coverage": "node node_modules/flow-coverage-report/bin/flow-coverage-report.js -i 'src/**/*.js' -t html -t json -t text --threshold 90",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:e2e": "testcafe firefox src/tests/*.e2e.js"
  },
  "devDependencies": {
    "flow-bin": "^0.133.0",
    "flow-coverage-report": "^0.7.0",
    "testcafe": "^1.9.2"
  }
}
